# MyLocalPlace - Environment Variables Example
# Copy this file to .env and fill with your actual values

# General
ENV=dev

# PostgreSQL Configuration (Server with multiple databases)
POSTGRES_USER=your-postgres-user
POSTGRES_PASSWORD=your-postgres-password
POSTGRES_PORT=5432

# PGAdmin Configuration
PGADMIN_DEFAULT_EMAIL=your-email@example.com
PGADMIN_DEFAULT_PASSWORD=your-pgadmin-password
PGADMIN_CONFIG_SERVER_MODE=False

# MySQL Configuration
MYSQL_DATABASE=mysqlDB
MYSQL_ROOT_PASSWORD=your-mysql-root-password
MYSQL_USER=your-mysql-user
MYSQL_PASSWORD=your-mysql-password
MYSQL_PORT=3306

# Redis Configuration
REDIS_PASSWORD=your-redis-password

# RabbitMQ Configuration
RABBITMQ_USER=admin
RABBITMQ_PASSWORD=your-rabbitmq-password

# MongoDB Configuration
ME_CONFIG_USERNAME=your-mongodb-user
ME_CONFIG_PASSWORD=your-mongodb-password

# LangFlow Configuration
LANGFLOW_DB=langflow_db
LANGFLOW_PORT=7860
LANGFLOW_SUPERUSER=your-langflow-user
LANGFLOW_SUPERUSER_PASSWORD=your-langflow-password

# Kafka Configuration
KAFKA_PORT=9092
ZOOKEEPER_PORT=2181
KAFDROP_PORT=19000

# OpenAI Configuration
OPENAI_API_KEY=sk-your-openai-key-here

# OpenWebUI Configuration
WEBUI_PORT=8082
WEBUI_AUTH=your-webui-username
WEBUI_NAME=Your Dashboard Name

# Jupyter Lab
JUPYTER_PORT=8888
JUPYTER_TOKEN=your-secure-token-here

# Jupyter Lab
JUPYTER_PORT=8888
JUPYTER_TOKEN=your-secure-token-here
JUPYTER_NOTEBOOKS_PATH=/your/path/to/notebooks
